{
  "name": "investing.dexe",
  "version": "0.1.0",
  "private": true,
  "homepage": "/",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "HTTPS=true SSL_CRT_FILE=./.cert/cert.pem SSL_KEY_FILE=./.cert/key.pem env-cmd -e development react-scripts start",
    "start-windows": "set HTTPS=true && set SSL_CRT_FILE=./.cert/cert.pem && set SSL_KEY_FILE=./.cert/key.pem && env-cmd -e development && react-scripts start",
    "build": "react-scripts build",
    "build:development": "env-cmd -e development yarn build",
    "build:staging": "env-cmd -e staging yarn build",
    "build-analyze": "yarn run build:development && yarn run analyze",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "c": "./node_modules/.bin/generate-react component",
    "r": "./node_modules/.bin/generate-react redux",
    "contracts:compile:abi": "typechain --target ethers-v5 --out-dir src/interfaces/typechain \"./src/abi/*.json\"",
    "postinstall": "yarn run contracts:compile:abi"
  },

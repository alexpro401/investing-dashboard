{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "styl",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component.tsx",
        "lazy": "templates/lazy.tsx"
      },
      "path": "src/components",
      "withStyle": false,
      "withTest": false,
      "withStory": true,
      "withLazy": false,
      "withAction": false
    }
  },
  "redux": {
    "default": {
      "path": "src/state",
      "withAction": true,
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false
    }
  }
}